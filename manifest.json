{
  "name": "Conv to 4 Spaces",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Tab indent Convert to 4 spacaes if GitLab.",
  "icons": {
    "32": "./capital_g.png"
  },
  "browser_action": {
    "default_title": "Conv to 4 Spaces (GitLab)"
  },
  "content_scripts": [
    {
      "js": ["./content_script.js"],
      "matches": ["http://gitlab.com/*", "https://gitlab.com/*"]
    }
  ]
}
